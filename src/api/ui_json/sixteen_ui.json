      {
        "customize": {
          "type": "text",
          "index_number": 0.44204858189620966,
          "name": "horizontal_text",
          "html_content": "<h4><span style=\"color:#327a8c\">针对不同的学员年龄，我们设置了以下课程：</span></h4>"
        },
        "advance": {
          "color": "",
